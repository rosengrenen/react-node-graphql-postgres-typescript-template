{
	"name": "@template-project/frontend",
	"version": "0.0.0",
	"repository": "",
	"main": "lib/index.js",
	"license": "MIT",
	"private": true,
	"scripts": {
		"build": "yarn codegen && parcel build src/index.html",
		"build:check": "yarn codegen && tsc --noEmit",
		"clean": "rimraf .cache dist **/__generated__",
		"codegen": "apollo client:codegen --target typescript",
		"codegen:watch": "apollo client:codegen --target typescript --watch",
		"dev": "concurrently --kill-others \"yarn codegen:watch\" \"yarn parcel:watch\"",
		"lint": "eslint --ext .ts,.tsx . && yarn prettier:check",
		"lint:fix": "eslint --ext .ts,.tsx --fix . && yarn prettier:format",
		"parcel:watch": "parcel -p 3000 --hmr-port 1234 --https watch src/index.html",
		"prettier:format": "prettier --write --ignore-path ../../.gitignore --loglevel error \"**/*.{js,jsx,json,yml,yaml}\"",
		"prettier:check": "prettier --list-different --ignore-path ../../.gitignore \"**/*.{js,jsx,json,yml,yaml}\"",
		"test": "jest --passWithNoTests"
	},
	"dependencies": {
		"@apollo/client": "^3.0.0-beta.43",
		"graphql": "^14.6.0",
		"query-string": "^6.12.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-router-dom": "^5.1.2"
	},
	"devDependencies": {
		"@types/react": "^16.9.25",
		"@types/react-dom": "^16.9.5",
		"@types/react-router-dom": "^5.1.4",
		"apollo": "^2.27.3",
		"concurrently": "^5.1.0",
		"eslint-plugin-react": "^7.19.0",
		"eslint-plugin-react-hooks": "^3.0.0",
		"parcel-bundler": "^1.12.4"
	}
}
